<?php
function calculateGrade($marks) {
    if ($marks >= 90) return "A";
    elseif ($marks >= 75) return "B";
    elseif ($marks >= 50) return "C";
    else return "F";
}

$students = ["Alice" => 85, "Bob" => 65, "Charlie" => 45];

foreach ($students as $name => $marks) {
    echo "$name: " . calculateGrade($marks) . "<br>";
}
?>
